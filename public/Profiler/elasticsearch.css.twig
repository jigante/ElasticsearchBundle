code {
    display: none;
}
code pre {
    padding: 5px;
    background-color: #DCDCDC;
}

ul.queries li h3 {
    margin: 0px 0px 7px;
}

ul.queries hr {
    border: 0;
    height: 0;
    border-top: 1px solid rgba(0, 0, 0, 0.1);
    border-bottom: 1px solid rgba(255, 255, 255, 0.3);
}

ul h3 i {
    font-style: normal;
    font-weight: 400;
    color: #313131
}

ul.queries li ul li {
    background-color: #f6f6f6;
    padding: 10px;
    margin: 0px 0px 5px 10px;
}

ul.queries li ul li div > div {
    margin: 5px 0px;
}
